{
  "name": "screenshot-renamer",
  "version": "1.0.0",
  "description": "A utility for thoughtful image renaming with clipboard integration",
  "main": "src/index.js",
  "bin": {
    "screenshot-renamer": "src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js start",
    "dev": "node src/cli.js start --dev",
    "setup": "node src/cli.js setup",
    "test": "node test-performance.js",
    "test:env": "node scripts/test-env.js",
    "install-service": "node src/cli.js install-service",
    "uninstall-service": "node src/cli.js uninstall-service",
    "batch-rename": "node src/batch-renamer.js"
  },
  "keywords": [
    "image",
    "rename",
    "screenshot",
    "ai",
    "automation"
  ],
  "author": "Max Milne",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmilneaus/Screenshot-Renamer.git"
  },
  "homepage": "https://github.com/maxmilneaus/Screenshot-Renamer#readme",
  "bugs": {
    "url": "https://github.com/maxmilneaus/Screenshot-Renamer/issues"
  },
  "dependencies": {
    "@google/generative-ai": "0.21.0",
    "chalk": "4.1.2",
    "chokidar": "4.0.1",
    "commander": "12.1.0",
    "inquirer": "8.2.6",
    "node-fetch": "^2.7.0",
    "ollama": "0.5.16",
    "pino": "9.5.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "pino-pretty": "13.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
